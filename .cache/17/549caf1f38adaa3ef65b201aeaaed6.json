        fetch(`http://localhost:4000/search?keyword=${this.keywod}&page=${page || this.page}`)\r\n            .then(res => res.json())\r\n            .then(json => {\r\n                // this.page = this.data.song.curpage\r\n               return json.data.song.list\r\n            } )\r\n            .then(songs => this.append(songs))\r\n    }\r\n    \r\n    append(songs) {\r\n        let html = songs.map(song => `\r\n          <li class=\"song-item\">\r\n            <i class=\"icon icon-music\"></i>\r\n            <div class=\"song-name ellipsis\">${song.songname}</div>\r\n            <div class=\"song-artist ellipsis\">${song.singer.map(s => s.name).join(' ')}</div>\r\n          </li>`).join('')\r\n        this.$songs.insertAdjacentHTML('beforeend', html)\r\n    }\r\n\r\n    //todo 搜索无实际内容，都是undefined\r\n}"},"lineCount":null}},"error":null,"hash":"f8d53f56447418b22d547507807e82e5","cacheData":{"env":{}}}